<div id="<%= dom_id publication %>">
  <p>
    <strong>Title:</strong>
    <%= publication.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= publication.description %>
  </p>

  <p>
    <strong>Label:</strong>
    <%= publication.label.name %>
  </p>


  <p>
    <strong>User:</strong>
    <%= link_to publication.user.email, '/users/:user_id/publications' %>
  </p>
</div>
